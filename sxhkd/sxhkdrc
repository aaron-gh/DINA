# Example bindings:
super + Return
	alacritty

super + a
	~/.local/bin/app-launcher

super + b
	~/.local/bin/toggle-blackout

super + BackSpace
	~/.local/bin/session-menu

# Volume controls
XF86AudioRaiseVolume
	pactl set-sink-volume @DEFAULT_SINK@ +5%

XF86AudioLowerVolume
	pactl set-sink-volume @DEFAULT_SINK@ -5%

XF86AudioMute
	pactl set-sink-mute @DEFAULT_SINK@ toggle
